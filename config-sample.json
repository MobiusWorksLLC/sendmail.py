[
  {
    "name": "Test FileSystem",
    "class": "FilesystemProcessor",
    "enabled" : true,
    "directory" : "../test-1/mail",
    "num_file" : "../test-1/num",
    "name_length" : 8
  },
  {
    "name": "SMTP Server",
    "class": "SmtpProcessor",
    "enabled" : true,
    "username": "SMTP_USERNAME",
    "password": "SMTP_PASSWORD",
    "host": "SMTP_SERVER",
    "port": 25,
    "to_address": "TO_ADDRESS",
    "from_address": false,
    "starttls": true
  },
  {
    "name": "Mandrill",
    "class": "MandrillProcessor",
    "enabled": true,
    "apikey": "MANDRILL_API_KEY",
    "to_address": "TO_ADDRESS",
    "from_address": "FROM_ADDRESS",
    "from_name": "FROM_NAME"
  },
  {
    "name": "Notification",
    "class": "NotifyProcessor",
    "enabled" : true,
    "icon_name": "mail-message-new",
    "title_template": "{appname} {name}",
    "text_template": "{subject}"
  },
  {
    "name": "Yagmail OAuth",
    "class": "YagmailProcessor",
    "enabled": true,
    "to_address": "TO_ADDRESS",
    "user": "GMAIL_USERNAME",
    "oauth2_file": "~/.yag_oauth"
  }
]
